<UserControl x:Class="CodenjoyBot.DataProvider.WebSocketDataProvider.WebSocketDataProviderDebugControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:webSocketDataProvider="clr-namespace:CodenjoyBot.DataProvider.WebSocketDataProvider">
    <Grid>
        <Label HorizontalAlignment="Center" VerticalAlignment="Center" Content="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type webSocketDataProvider:WebSocketDataProviderDebugControl}}, Path=DataProvider.IdentityUser}"/>
    </Grid>
</UserControl>
